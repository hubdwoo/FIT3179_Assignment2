{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": "container",
    "height": "container",
    "background": "white",
    "data": 
    {
        "url": "https://raw.githubusercontent.com/hubdwoo/FIT3179_Week9/main/Datasets/state_without_date.csv"
    },
    "mark":{
        "type": "point",
        "filled": true
    },
    "encoding": 
    {
        "x":
        {
            "field": "positive",
            "type": "quantitative",
            "axis":
            {
                "grid": false
            }
        },
        "y":
        {
            "field": "negative",
            "type": "quantitative",
            "axis": 
            {
                "grid": false
            }
        },
        "size":
        {
            "field": "POPESTIMATE2020",
            "type": "quantitative",
            "scale": {
                "range": [10, 2000]
            }
        },
        "color":
        {
            "field": "City",
            "type": "nominal",
            "scale": {
                "scheme": "set1"
            }
        },
        "tooltip": [{
            "field": "City",
            "type": "nominal",
            "title": "State"
        },{
            "field": "positive",
            "type": "quantitative",
            "title": "Positive tests",
            "format": ","
        },{
            "field": "negative",
            "type": "quantitative",
            "title": "Negative tests",
            "format": ","
        },{
            "field": "POPESTIMATE2020",
            "type": "quantitative",
            "title": "Population",
            "format": ","
        }]
    },
    "config": {
        "legend": {
            "disable": true
        },
        "view": {
            "stroke": "transparent"
        }
    }
}